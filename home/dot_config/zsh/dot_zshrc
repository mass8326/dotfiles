# omz setup

export ZSH="$ZDOTDIR/omz"
export ZSH_CUSTOM="$ZDOTDIR/custom"

ZSH_THEME="mass"

plugins=(
  # Quality of life
  zsh-syntax-highlighting
  zsh-autosuggestions
  you-should-use
  vi-mode
  # Utilities
  chezmoi
  git
  # Languages
  nvm
  golang
  rust
)

source $ZSH/oh-my-zsh.sh

[ -f "$HOME/.zshrc.local" ] && source "$HOME/.zshrc.local"

alias pg="pgrep -a"
alias pk="pkill"
