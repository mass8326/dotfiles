# omz setup

export ZSH="$HOME/.config/omz/master"
export ZSH_CUSTOM="$HOME/.config/omz/custom"

ZSH_THEME="mass"

plugins=(
  composer
  git
  zsh-autosuggestions
  you-should-use
  nvm
  vi-mode
  fancy-ctrl-z
  golang
  rust
)

source $ZSH/oh-my-zsh.sh

[ -f "$HOME/.zshrc.local" ] && source "$HOME/.zshrc.local"

alias pg="pgrep -a"
alias pk="pkill"
